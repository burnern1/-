public class Solution {
    public String solution(String[] seoul) {
        // "Kim"의 위치를 찾기 위한 변수
        int index = -1;
        
        // 배열을 순회하면서 "Kim"의 위치를 찾는다
        for (int i = 0; i < seoul.length; i++) {
            if (seoul[i].equals("Kim")) {
                index = i;
                break;
            }
        }
        
        // 결과 문자열을 반환
        return "김서방은 " + index + "에 있다";
    }
}
